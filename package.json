{
  "name": "nh3-lab",
  "version": "0.1.0",
  "author": "imagoiq",
  "description": "Home page referencing lab experiences",
  "scripts": {
    "start": "npm run eleventy-build && npm run build-css",
    "eleventy-build": "eleventy --input=src --output=dist --formats=liquid,md",
    "build-css": "node-sass --output-style compressed --include-path scss src/assets/style/main.scss   dist/assets/style/main.css"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "dotenv": "^8.0.0",
    "html-minifier": "^4.0.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1"
  }
}
